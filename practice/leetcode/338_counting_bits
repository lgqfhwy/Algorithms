// https://leetcode-cn.com/problems/counting-bits/

class Solution {
public:
    vector<int> countBits(int n) {
        vector<int> ret;
        for (int i = 0; i <= n; ++i) {
            int cur = compute_bits(i);
            ret.push_back(cur);
        }
        return ret;
    }
    int compute_bits(int i) {
        int ret = 0;
        while (i != 0) {
            ret += (i & 1);
            i >>= 1;
        }
        return ret;
    }
};